{
  "name": "UrStyle",
  "version": "1.0.0",
  "description": "Capstone Project React Native, based on Soft UI React Native Design. Coded by TuanKiet0904",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx --quiet",
    "format-code": "prettier --single-quote --semi false --trailing-comma all --tab-width 2 --write \"**/*.{tsx,js,css,scss}\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/drawer": "5.12.5",
    "@react-navigation/native": "5.9.4",
    "@react-navigation/stack": "5.14.5",
    "axios": "^1.3.6",
    "dayjs": "1.10.5",
    "expo": "48.0.0",
    "expo-app-loading": "2.0.0",
    "expo-blur": "~12.2.2",
    "expo-constants": "~14.2.1",
    "expo-haptics": "~12.2.1",
    "expo-image-picker": "~14.1.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-localization": "~14.1.1",
    "expo-modules-core": "~1.2.7",
    "expo-status-bar": "~1.4.4",
    "i18n-js": "3.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-native": "0.71.7",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-keyboard-aware-scroll-view": "0.9.4",
    "react-native-mime-types": "^2.4.0",
    "react-native-pager-view": "6.1.2",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-web": "~0.18.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/i18n-js": "3.8.1",
    "@types/react": "~18.0.27",
    "@types/react-native": "0.67.6",
    "eslint": "7.28.0",
    "jest-expo": "48.0.0",
    "prettier": "^2.8.7",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Capstone-Project-UrStyle/Mobile-app.git"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ]
  },
  "keywords": [],
  "author": "Tuan Kiet <tuankietnk2001@gmail.com>"
}
